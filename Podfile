# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Shot Sample iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Shot Sample iOS
  pod 'Moya'
  pod 'SwiftyJSON'
  pod 'Kingfisher', '~> 4.0'

  target 'Shot Sample iOSTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Moya'
    pod 'SwiftyJSON'
    pod 'Kingfisher', '~> 4.0'
  end

  target 'Shot Sample iOSUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Moya'
    pod 'SwiftyJSON'
    pod 'Kingfisher', '~> 4.0'
  end

end
